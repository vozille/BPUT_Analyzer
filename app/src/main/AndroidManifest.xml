<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vozille.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DispalayMsgActivity"
            android:label="@string/title_activity_dispalay_msg"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".aboutapp"
            android:label="@string/title_activity_aboutapp"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".compare_home"
            android:label="@string/title_activity_compare_home"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".history"
            android:label="@string/title_activity_history"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".rank_sem"
            android:label="@string/title_activity_rank_sem"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".display_rank_list"
            android:label="@string/title_activity_display_rank_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".rank_history"
            android:label="@string/title_activity_rank_history"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
